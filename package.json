{
  "name": "blog",
  "version": "0.1.156",
  "homepage": "https://philp.io",
  "repository": "https://github.com/chrismphilp/philp.io",
  "description": "My personal blog",
  "author": "Chris Philp",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.tsx\"",
    "dev": "next dev",
    "patch": "npm version --no-commit-hooks --no-git-tag-version patch && git add package.json package-lock.json && git commit -m \"[main] | Bumping version\"",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "npx serve@latest out",
    "static": "next build && next-image-export-optimizer",
    "lint": "eslint ."
  },
  "pre-push": [
    "format",
    "lint",
    "patch"
  ],
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.17.0"
  },
  "resolutions": {
    "vscode-textmate": "8.0.0"
  },
  "dependencies": {
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.4.9",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "^22.14.1",
    "autoprefixer": "10.4.14",
    "gray-matter": "4.0.3",
    "next": "^15.5.4",
    "next-image-export-optimizer": "1.12.3",
    "next-mdx-remote": "4.4.1",
    "next-pwa": "^5.6.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga": "3.3.1",
    "react-icons": "4.10.1",
    "react-markdown": "^8.0.7",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-img-size": "1.0.1",
    "rehype-pretty-code": "0.9.11",
    "rehype-shiki": "0.0.9",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1",
    "sass": "1.63.6",
    "sharp": "0.33.3",
    "shiki": "0.14.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.33",
    "@types/minimatch": "^5.1.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "next-sitemap": "^4.2.3",
    "pre-push": "^0.1.4",
    "prettier": "3.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "^5.8.3",
    "vercel": "^28.16.15"
  }
}
